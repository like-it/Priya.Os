<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/org/gnome/gedit/plugins/wordcompletion/" id="org.gnome.gedit.plugins.wordcompletion" gettext-domain="gedit-plugins">
    <key type="b" name="interactive-completion">
      <default>true</default>
      <summary>Interactive Completion</summary>
      <description>Whether to enable interactive completion.</description>
    </key>
    <key type="u" name="minimum-word-size">
      <range min="2" max="100"/>
      <default>2</default>
      <summary>Minimum Word Size</summary>
      <description>The minimum word size to complete.</description>
    </key>
  </schema>
</schemalist>