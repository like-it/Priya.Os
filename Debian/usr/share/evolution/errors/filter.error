<?xml version="1.0" encoding="UTF-8"?>
<error-list domain="filter">

 <error type="error" id="no-date">
  <primary>Missing date.</primary>
  <secondary>You must choose a date.</secondary>
 </error>

 <error type="error" id="no-file">
  <primary>Missing filename.</primary>
  <secondary>You must specify a filename.</secondary>
 </error>

 <error type="error" id="bad-file">
  <primary>File &quot;{0}&quot; does not exist or is not a regular file.</primary>
  <secondary>You must specify a filename.</secondary>
 </error>

 <error type="error" id="bad-regexp">
  <primary>Bad regular expression &quot;{0}&quot;.</primary>
  <secondary>Could not compile regular expression &quot;{1}&quot;.</secondary>
 </error>

 <error type="error" id="no-name">
  <primary>Missing name.</primary>
  <secondary>You must name this filter.</secondary>
 </error>

 <error type="error" id="bad-name-notunique">
  <primary>Name &quot;{0}&quot; already used.</primary>
  <secondary>Please choose another name.</secondary>
 </error>

 <error type="error" id="no-condition">
  <primary>Missing condition.</primary>
  <secondary>Filter should have at least one condition.</secondary>
 </error>

 <error type="error" id="not-allow-empty">
  <primary>Missing value.</primary>
  <secondary>One or more values cannot be empty.</secondary>
 </error>

 <error type="question" id="remove-rule-question" default="GTK_RESPONSE_YES">
  <primary>Are you sure you want to remove rule '{0}'?</primary>
  <button response="GTK_RESPONSE_CANCEL" label="_Keep"></button>
  <button response="GTK_RESPONSE_YES" label="_Remove"></button>
 </error>
</error-list>